#!/bin/bash

if [ -z "${hisitopdir}" ]; then export hisitopdir=$(pwd)/../..; fi
. ${hisitopdir}/autelan_scripts/autelan.apps

package_common "$(pwd)"

rm -fr ${autefake}/usr/local/include
mv ${auterelease}/usr/local/include ${autefake}/usr/local/include

dir=${auterelease}/usr/local
RM ${dir}/lib/engines
RM ${dir}/lib/pkgconfig
RM ${dir}/ssl/man
RM ${dir}/lib/libcrypto.a
RM ${dir}/lib/libssl.a
mv ${dir}/lib/libssl* ${auterelease}/usr/lib/
mv ${dir}/lib/libcrypto* ${auterelease}/usr/lib/


